paises = {'colombia', 'peru', 'chile', 'aruba', 'Brasil', 'Australia'}

paises2 = {'España','Japón','Brasil','Egipto','Canadá','colombia'}

# union de conjuntos 

paises3 = paises.union(paises2)
'''
print (paises3)

print (paises | paises2)
'''
# interseccion de conjuntos

print(paises.intersection(paises2))
print(paises & paises2)

# diferencia         nos da los elementos que no se repiten 

print (paises.difference(paises2))
print (paises - paises2)

# diferencia simetrica  incluye los elementos que son exclusivos de cada conjunto y excluye aquellos que son comunes a ambos.

print(paises.symmetric_difference(paises2))
print(paises^paises2)